{
  "name": "chatgpbot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "dev:docker": "docker-compose build --no-cache && docker-compose up --build",
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "route:list": "npx ts-node ./src/Infrastructure/Command/listRoutes.ts",
    "serve": "node build/index.js",
    "lint": "eslint 'src/**/*.ts' --quiet",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.{ts,js,json}'",
    "migration:create": "typeorm migration:create ./src/Infrastructure/Database/migration/$npm_config_migration",
    "migrate": "npm run typeorm migration:run",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/Infrastructure/Config/database.config.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^8.13.1",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "openai": "^4.29.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mysql": "^2.15.26",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "bcrypt": "^5.1.1",
    "eslint": "^8.57.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "typeorm-typedi-extensions": "^0.4.1",
    "typescript": "^5.4.2"
  }
}
